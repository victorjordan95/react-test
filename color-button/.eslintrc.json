{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "testing-library",
    "jest-dom"
  ],
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:testing-library/react",
    "plugin:jest-dom/react"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-throw-literal": "off",
    "no-useless-escape": "off",
    "semi": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off",
    "multiline-ternary": "off",
    "space-before-function-paren": "off",
    "react/display-name": "off",
    "no-debugger": 0,
    "indent": "off"
  }
}